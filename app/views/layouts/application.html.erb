<!DOCTYPE html>
<html>
  <head>
    <title><%= @title %></title>
    <%= csrf_meta_tag %>
    <%= render 'layouts/stylesheets' %>
    <%= javascript_include_tag 'jquery' %>
    <%= javascript_include_tag 'jquery-ui.custom.js' %>
    <%= javascript_include_tag 'jquery.flot.min.js' %>
    <%= javascript_include_tag 'jquery.dynatree.js' %>
    <%= javascript_include_tag 'jquery-ui-timepicker-addon.js' %>
    <%= javascript_include_tag :defaults %>
  </head>
  <body>
    <%= render 'layouts/header' %>
    <% flash.each do |key, value| %>
      <div class="flash <%= key %>"><%= value %></div>
    <% end %>
    <div class="content">
      <%= yield %>
    </div>
    <%= render 'layouts/footer' %>
  <% if Rails.env.development? %>
    <div id="debug"><%= debug(params) %></div>
  <% end %>
  </body>
</html>
